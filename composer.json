{
    "name": "codeium/windsurf-project",
    "description": "WordPress security plugin for malware detection and prevention",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "ext-pcre": "*",
        "ext-mbstring": "*",
        "composer/installers": "^1.0",
        "micropackage/requirements": "^1.2",
        "wpbp/debug": "~1.1.0",
        "wpbp/cronplus": "~1.0.0",
        "wpbp/template": "~1.0.0",
        "wpbp/widgets-helper": "~1.0.0",
        "wpbp/page-madness-detector": "^1.0",
        "wpdesk/wp-notice": "^3.2",
        "yahnis-elsts/plugin-update-checker": "^5.4",
        "cmb2/cmb2": "~2.10.0",
        "johnbillion/extended-cpts": "~5.0.8",
        "stevegrunwell/wp-cache-remember": "dev-master",
        "monolog/monolog": "^2.9|^3.0",
        "symfony/cache": "^5.4|^6.0",
        "symfony/filesystem": "^5.4|^6.0",
        "symfony/finder": "^5.4|^6.0",
        "guzzlehttp/guzzle": "^7.5",
        "psr/log": "^1.1|^2.0|^3.0",
        "psr/cache": "^1.0|^2.0|^3.0",
        "psr/simple-cache": "^1.0|^2.0|^3.0",
        "league/flysystem": "^3.0",
        "ramsey/uuid": "^4.7"
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "1.0.0",
        "wp-coding-standards/wpcs": "^3.0",
        "phpunit/phpunit": "^9.2",
        "phpro/grumphp": "^1.13",
        "szepeviktor/phpstan-wordpress": "^1.1.6",
        "codeception/codeception": "^4.2|^5.0",
        "codeception/module-asserts": "^1.0",
        "codeception/module-cli": "^1.0",
        "codeception/module-db": "^1.0",
        "codeception/module-filesystem": "^1.0",
        "codeception/module-phpbrowser": "^1.0",
        "codeception/module-rest": "^1.2",
        "codeception/module-webdriver": "^1.0",
        "lucatume/wp-browser": "^3.6",
        "php-stubs/wordpress-stubs": "^6.2",
        "php-stubs/wordpress-tests-stubs": "^6.2",
        "php-stubs/wp-cli-stubs": "^2.0",
        "ergebnis/composer-normalize": "^2.6",
        "mockery/mockery": "^1.6",
        "fakerphp/faker": "^1.23",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-phpunit": "^1.3",
        "phpstan/phpstan-strict-rules": "^1.5",
        "phpbench/phpbench": "^1.2",
        "symfony/var-dumper": "^5.4|^6.0",
        "phpunit/php-code-coverage": "^9.2",
        "squizlabs/php_codesniffer": "^3.7",
        "infection/infection": "^0.27"
    },
    "autoload": {
        "classmap": [
            "includes/",
            "admin/"
        ],
        "files": [
            "includes/class-oms-config.php",
            "includes/class-oms-logger.php",
            "includes/class-oms-utils.php",
            "includes/class-oms-exception.php",
            "includes/class-oms-cache.php",
            "includes/class-oms-rate-limiter.php",
            "includes/class-file-security-policy.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpro/grumphp": true,
            "ergebnis/composer-normalize": true,
            "infection/extension-installer": true
        },
        "platform": {
            "php": "8.1"
        },
        "optimize-autoloader": true,
        "sort-packages": true,
        "preferred-install": {
            "*": "dist"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:unit": "phpunit --testsuite default",
        "test:coverage": "phpunit --coverage-html coverage",
        "test:codeception": "codecept run",
        "test:codeception:unit": "codecept run unit",
        "test:codeception:functional": "codecept run functional",
        "test:codeception:acceptance": "codecept run acceptance",
        "test:mutation": "infection --threads=4 --min-msi=70 --min-covered-msi=80",
        "test:all": [
            "@test",
            "@test:codeception"
        ],
        "phpcs": "phpcs --standard=WordPress",
        "phpcs:report": "phpcs --standard=WordPress --report=summary",
        "phpcbf": "phpcbf --standard=WordPress",
        "phpstan": "phpstan analyse --memory-limit=1G",
        "phpstan:baseline": "phpstan analyse --generate-baseline",
        "lint": [
            "@phpcs",
            "@phpstan"
        ],
        "lint:fix": "@phpcbf",
        "bench": "phpbench run --report=default",
        "bench:compare": "phpbench run --report=default --ref=baseline",
        "check": [
            "@lint",
            "@test:all"
        ],
        "check:all": [
            "@lint",
            "@test:all",
            "@bench"
        ],
        "post-install-cmd": [
            "@php -r \"if (!file_exists('vendor/bin/phpcs')) { echo 'Installing code standards...\\n'; }\"",
            "@php -r \"echo 'Run composer install completed successfully.\\n';\""
        ],
        "post-update-cmd": [
            "@php -r \"echo 'Dependencies updated successfully.\\n';\""
        ],
        "post-autoload-dump": [
            "@php -r \"echo 'Autoloader dumped successfully.\\n';\""
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
